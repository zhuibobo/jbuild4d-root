<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jbuild4d.base.dbaccess.dao.systemsetting.SettingMapper">
    <select id="selectAll" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from TB4D_SETTING ORDER by SETTING_CREATETIME DESC
    </select>
    <select id="nextOrderNum" resultType="integer">select case when max(SETTING_ORDER_NUM) is null then 1 else max(SETTING_ORDER_NUM)+1 end ORDERNUM from TB4D_SETTING</select>
    <select id="selectBySearch" parameterType="java.util.Map" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from TB4D_SETTING ORDER by SETTING_CREATETIME DESC
    </select>
    <delete id="deleteAll">
        delete from TB4D_SETTING
    </delete>

    <select id="selectByKeyField" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from TB4D_SETTING where SETTING_KEY=#{key,jdbcType=NVARCHAR}
    </select>
</mapper>