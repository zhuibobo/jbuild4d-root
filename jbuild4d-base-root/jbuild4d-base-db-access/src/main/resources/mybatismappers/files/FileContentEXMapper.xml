<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jbuild4d.base.dbaccess.dao.files.FileContentMapper">
    <select id="selectAll" resultMap="BaseResultMap">select<include refid="Base_Column_List"/> from TB4D_FILE_CONTENT ORDER by  DESC</select>
    <delete id="deleteAll">delete from TB4D_FILE_CONTENT</delete>
    <select id="nextOrderNum" resultType="integer">select case when max() is null then 1 else max()+1 end ORDERNUM from TB4D_FILE_CONTENT</select>
    <select id="selectBySearch" parameterType="java.util.Map" resultMap="BaseResultMap">select<include refid="Base_Column_List"/>from TB4D_FILE_CONTENT<where/>ORDER by  DESC</select>
    <select id="selectLessThanRecord" parameterType="java.lang.String" resultMap="BaseResultMap"><![CDATA[select * from TB4D_FILE_CONTENT
                                                                                                          where  = (select max() from TB4D_FILE_CONTENT where <(select  from TB4D_FILE_CONTENT where FILE_ID=#{Id,jdbcType=NVARCHAR}))]]></select>
    <select id="selectGreaterThanRecord" parameterType="java.lang.String" resultMap="BaseResultMap"><![CDATA[select * from TB4D_FILE_CONTENT
                                                                                                             where  = (select min() from TB4D_FILE_CONTENT where >(select  from TB4D_FILE_CONTENT where FILE_ID=#{Id,jdbcType=NVARCHAR}))]]></select>
</mapper>
