{
    "presets": [
        ["env", {
            "loose": false,
            "modules": "commonjs",
            "spec": true,
            "targets": {
                "browsers": [
                    "last 3 Chrome versions",
                    "last 3 Firefox versions",
                    "Safari >= 10",
                    "Explorer >= 10",
                    "Edge >= 12",
                    "iOS >= 10",
                    "Android >= 6"
                ]
            },
            "useBuiltIns": "usage",
            "debug": false
        }],
        "stage-3"
    ],
    "presets": [
        "es2015"
    ],
    "plugins": [
        "dynamic-import-node",
        "transform-class-properties",
        "transform-es5-property-mutators",
        "transform-object-rest-spread",
        "transform-runtime"
    ],
    "comments": false
}
