{
    "presets": [
        ["env", {
            "loose": false,
            "modules": "commonjs",
            "spec": true,
            "targets": {
                "browsers": [
                    "last 3 Chrome versions",
                    "last 3 Firefox versions",
                    "Safari >= 10",
                    "Explorer >= 10",
                    "Edge >= 12",
                    "iOS >= 10",
                    "Android >= 6"
                ]
            },
            "useBuiltIns": "usage",
            "debug": false
        }],
        "stage-3"
    ],
    "plugins": [
        "dynamic-import-node-babel-7"/*,
        "@babel/transform-class-properties",
        "@babel/transform-es5-property-mutators",
        "@babel/transform-object-rest-spread",
        "@babel/transform-runtime"*/
    ],
    "comments": false
}
